<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/imd_food1"
        android:layout_width="143dp"
        android:layout_height="129dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/name_food1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.066"
        app:srcCompat="@drawable/image_13" />

    <TextView
        android:id="@+id/name_food1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="176dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imd_food1" />

    <TextView
        android:id="@+id/price_food1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/imd_food1"
        app:layout_constraintStart_toStartOf="@+id/decr_food1"
        app:layout_constraintTop_toBottomOf="@+id/decr_food1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/decr_food1"
        android:layout_width="219dp"
        android:layout_height="36dp"
        android:layout_marginTop="24dp"
        android:lines="2"
        android:minLines="2"
        android:singleLine="false"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/name_food1"
        app:layout_constraintTop_toBottomOf="@+id/name_food1" />
</androidx.constraintlayout.widget.ConstraintLayout>